   "source": [
    "regr = ElasticNetCV(l1_ratio=[.1, .5, .7, .9, .95, .99, 1],#float between 0 and 1 passed to ElasticNet (scaling between l1 and l2 penalties)\n",
    "                   eps=0.001,#Length of the path\n",
    "                   n_alphas=200,#Number of alphas along the regularization path\n",
    "                   alphas=None,#alphas are set automatically\n",
    "                   fit_intercept=True,#data is not expected to be centered\n",
    "                   normalize=True,#the regressors will be normalized before regression\n",
    "                   precompute='auto',max_iter=1000,#set as defult\n",
    "                   tol=0.0001,#The tolerance for the optimization\n",
    "                   cv=None,# use the default 5-fold cross-validation\n",
    "                   copy_X=True, verbose=0,#set as defult\n",
    "                   n_jobs=None,#1 CPU is used during the cross validation.\n",
    "                   positive=False,# all the coefficients are not to be positive\n",
    "                   random_state=0,\n",
    "                   selection='random'# a random coefficient is updated every iteration\n",
    "                   )"
   ]
  },
  {
   "cell_type": "code",
