    "                  max_iter=1000, #set as defult\n",
    "                  copy_X=True, tol=0.0001, #set as hyper parameter tuning\n",
    "                  warm_start=False, #erase the previous solution\n",
    "                  positive=False, random_state=0,#set as hyper parameter tuning\n",
    "                  selection='random'#set as hyper parameter tuning\n",
